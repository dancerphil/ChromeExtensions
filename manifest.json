{
	"manifest_version": 3,
	"name": "Open extension API reference",
	"version": "1.0.0",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"tabs",
		"tabGroups",
		"commands"
	],
	"background": {
		"service_worker": "service-worker.js",
		"type": "module"
	},
	"commands": {
		"split": {
			"suggested_key": {
				"default": "Ctrl+E",
				"mac": "Command+E"
			},
			"description": "分离标签"
		}
	}
}
