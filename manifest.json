{
	"manifest_version": 2,
	"name": "Zhihu Blocker",
	"description": "Web版知乎。屏蔽黑名单用户发布的内容。[ www.zhihu.com] 源码：https://github.com/dancerphil/ZhihuBlocker",
	"permissions": [
		"http://*.zhihu.com/",
		"https://*.zhihu.com/",
		"http://www.bilibili.com/",
		"http://scp-wiki-cn.wikidot.com/",
		"https://tour.go-zh.org/"
	],
	"content_scripts": [{
		"matches": [
			"http://www.zhihu.com/*",
			"https://www.zhihu.com/*",
			"http://www.bilibili.com/*",
			"http://scp-wiki-cn.wikidot.com/*",
			"https://tour.go-zh.org/*"
		],
		"js": [
			"jquery.min.js",
			"left-pad.js",
			"ZhihuBlocker.js",
			"FollowerBlocker.js",
			"Bilibili.js",
			"scp.js",
			"go.js"]
	}],
	"version": "1.0.7"
}
